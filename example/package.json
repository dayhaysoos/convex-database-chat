{
  "name": "ecommerce-chat-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:ui\" \"npm run convex\"",
    "dev:ui": "vite --port 3000 --strictPort",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "serve dist -s -l ${PORT:-3000}",
    "convex": "convex dev",
    "seed": "convex run seed:seedProducts",
    "seed:embeddings": "convex run seed:generateProductEmbeddings"
  },
  "dependencies": {
    "@dayhaysoos/convex-database-chat": "^0.2.0-alpha.1",
    "@fingerprintjs/fingerprintjs": "^4.5.1",
    "convex": "^1.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.5.0",
    "vite": "^7.3.0"
  }
}
